#: ajax/clipboard.php:48
#, php-format
msgid "Could not remove '%s'"
msgstr "'%s' konnte nicht entfernt werden."

#: ajax/clipboard.php:55
#, php-format
msgid "Could not move '%s'"
msgstr "'%s' konnte nicht verschoben werden."

#: ajax/clipboard.php:59
#, php-format
msgid "Could not copy '%s'"
msgstr "'%s' konnte nicht kopiert werden."

#: js/clipboard.js:11
msgid "Cut"
msgstr "Ausschneiden"

#: js/clipboard.js:19
msgid "Copy"
msgstr "Kopieren"

#: js/clipboard.js:26 js/clipboard.js:165
msgid "Paste"
msgstr "Einfügen"

#: js/clipboard.js:63
msgid "Unable to paste: the files come from this directory."
msgstr "Einfügen nicht möglich: die Dateien stammen aus diesem Verzeichnis."

#: js/clipboard.js:64
msgid ""
"Unable to paste: you do not have the permissions to create files in this "
"directory."
msgstr ""
"Einfügen nicht möglich: sie haben keine Berechtigung Dateien in diesem "
"Verzeichis zu erstellen."

#: js/clipboard.js:65
#, c-format
msgid "Paste %n item"
msgid_plural "Paste %n items"
msgstr[0] "Ein Element einfügen"
msgstr[1] "%n Elemente einfügen"

#: js/clipboard.js:131
msgid "An error occurred during the move."
msgstr "Beim verschieben ist ein Fehler aufgetreten."

#: js/clipboard.js:132
msgid "An error occurred during the copy."
msgstr "Beim kopieren ist ein Fehler aufgetreten."

#: js/clipboard.js:153
msgid "Processing. Leaving the page now will interrupt the move."
msgstr ""
"Verarbeitung: Wird die Seite jetzt verlassen, wird das Verschieben "
"abgebrochen."

#: js/clipboard.js:154
msgid "Processing. Leaving the page now will interrupt the copy."
msgstr ""
"Verarbeitung: Wird die Seite jetzt verlassen, wird das Kopieren abgebrochen."

#: js/clipboard.js:164
msgid ""
"The contents of the clipboard is in conflicts with elements already present "
"in this directory. Do you want to replace them ?"
msgstr ""
"Der Inahlt der Zwischenablage ist in diesem Verzeichnis bereits vorhanden. "
"Soll er ersetzt werden?"
