#: ajax/clipboard.php:48
#, php-format
msgid "Could not remove '%s'"
msgstr "Impossible de supprimer '%s'."

#: ajax/clipboard.php:55
#, php-format
msgid "Could not move '%s'"
msgstr "Impossible de déplacer '%s'."

#: ajax/clipboard.php:59
#, php-format
msgid "Could not copy '%s'"
msgstr "Impossible de copier '%s'."

#: js/clipboard.js:11
msgid "Cut"
msgstr "Couper"

#: js/clipboard.js:19
msgid "Copy"
msgstr "Copier"

#: js/clipboard.js:26 js/clipboard.js:165
msgid "Paste"
msgstr "Coller"

#: js/clipboard.js:63
msgid "Unable to paste: the files come from this directory."
msgstr "Impossible de coller : les fichiers proviennent de ce répertoire."

#: js/clipboard.js:64
msgid ""
"Unable to paste: you do not have the permissions to create files in this "
"directory."
msgstr ""
"Impossible de coller : vous n'avez pas les permissions pour créer des "
"fichiers dans ce répertoire."

#: js/clipboard.js:65
msgid "Paste %n item"
msgid_plural "Paste %n items"
msgstr[0] "Coller un élément"
msgstr[1] "Coller %n éléments"

#: js/clipboard.js:131
msgid "An error occurred during the move."
msgstr "Une erreur s'est produite lors du déplacement."

#: js/clipboard.js:132
msgid "An error occurred during the copy."
msgstr "Une erreur s'est produite lors de la copie."

#: js/clipboard.js:153
msgid "Processing. Leaving the page now will interrupt the move."
msgstr ""
"Traitement en cours. Quitter la page maintenant va interrompre le "
"déplacement."

#: js/clipboard.js:154
msgid "Processing. Leaving the page now will interrupt the copy."
msgstr ""
"Traitement en cours. Quitter la page maintenant va interrompre la copie."

#: js/clipboard.js:164
msgid ""
"The contents of the clipboard is in conflicts with elements already present "
"in this directory. Do you want to replace them ?"
msgstr ""
"Le contenu du presse-papier entre en conflit avec des éléments déjà présent "
"à cet emplacement. Souhaitez-vous les remplacer ?"
