#: ajax/clipboard.php:48
#, php-format
msgid "Could not remove '%s'"
msgstr "Nem lehet eltávolítani '%s'"

#: ajax/clipboard.php:55
#, php-format
msgid "Could not move '%s'"
msgstr "Nem mozgatható '%s'"

#: ajax/clipboard.php:59
#, php-format
msgid "Could not copy '%s'"
msgstr "Nem másolható '%s'"

#: js/clipboard.js:11
msgid "Cut"
msgstr "Kivágás"

#: js/clipboard.js:19
msgid "Copy"
msgstr "Másolás"

#: js/clipboard.js:26 js/clipboard.js:165
msgid "Paste"
msgstr "Beillesztés"

#: js/clipboard.js:63
msgid "Unable to paste: the files come from this directory."
msgstr "Nem illeszthető be: a fájlok ebből a könyvtárból származnak"

#: js/clipboard.js:64
msgid ""
"Unable to paste: you do not have the permissions to create files in this "
"directory."
msgstr "Nem illeszthető be: nincs jogosultsága hozzá"

#: js/clipboard.js:65
msgid "Paste %n item"
msgid_plural "Paste %n items"
msgstr[0] "Fájl beillesztése"
msgstr[1] "%n db. Fájl beillesztése"

#: js/clipboard.js:131
msgid "An error occurred during the move."
msgstr "Hiba történt az áthelyezés során"

#: js/clipboard.js:132
msgid "An error occurred during the copy."
msgstr "Hiba történt a másolás során"

#: js/clipboard.js:153
msgid "Processing. Leaving the page now will interrupt the move."
msgstr "Feldolgozás. Az oldal elhagyása megszakítja a mozgatást"

#: js/clipboard.js:154
msgid "Processing. Leaving the page now will interrupt the copy."
msgstr "Feldolgozás. Az oldal elhagyása megszakítja a másolást"

#: js/clipboard.js:164
msgid ""
"The contents of the clipboard is in conflicts with elements already present "
"in this directory. Do you want to replace them ?"
msgstr ""
"A vágólap tartalma ütközik a már meglévő elemekkel"
"ebben a könyvtárban. Szeretnéd cserélni?"
